<%
  cases = overview_head
  case_class = cases[0].classification
  class_group = case_class.classification_group
%>

<tr class="PAP_<%=h case_class.code %>" style="font-weight: bold; border: 1px">
  <th colspan="2" style="font-size: 0.9em; font-weight: bold; padding: 0 0.5em; border: 0px" ><%=h class_group.title %> <i style="font-size: 0.8em; font-weight: normal;">(<%= cases.size %>)</i></th>
  <th style="text-align: left; font-weight: normal; padding: 0 1em; border: 0px"><i>Entn.</i></th>
  <th style="border: 0px"></th>
</tr>
